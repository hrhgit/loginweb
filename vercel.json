{
  "rewrites": [
    {
       "source": "/((?!assets|pages|chunks|icons|fonts|sw\\.js|background-worker\\.js|vite\\.svg|.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
"key": "Cache-Control",
"value": "public, max-age=31536000, immutable"
}
]
},
{
"source": "/(pages|chunks)/(.*)\\.js",
"headers": [
{
"key": "Content-Type",
 "value": "application/javascript; charset=utf-8"
},
{
 "key": "Cache-Control",
 "value": "public, max-age=31536000, immutable"
}
]
},
{
"source": "/sw.js",
"headers": [
{
"key": "Content-Type",
 "value": "application/javascript; charset=utf-8"
},
{
 "key": "Cache-Control",
"value": "public, max-age=0, must-revalidate"
 }
]
}
]
}